#import <stdio.h>
#import <stdlib.h>

int main(int argc, char** argv){
	int numPoints = 0;
	sscanf(argv[1],"%d", &numPoints);
	double xVals[numPoints];
	double yVals[numPoints];

	for(int i = 0; i < numPoints; i++){
		double temp = atof(argv[i + 2]);
		if((temp % 2) == 0){
			xVals[i] = temp);
		} else {
			yVals[i] = temp);
		}
	}

	printf("\n%d", numPoints);
}
