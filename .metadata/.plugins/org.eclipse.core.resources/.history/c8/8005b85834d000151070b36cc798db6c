#import <stdio.h>

int main(int argc, char** argv){
	int numPoints = 0;
	sscanf(argv[1],"%d", &numPoints);
	double points[2][numPoints];

	for(int i = 0; i < 2; i++){
		for(int j = 0; j < numPoints; j++){
			sscanf(argv[j + 2], "%lf", &points[i][j]);
		}
	}
	for(int i = 0; i < numPoints; i++){
			printf("%lf %lf\n", points[0][i], points[1][i]);
		}
	printf("\n%d", numPoints);
}
